{
  "workbench.colorCustomizations": {
    "titleBar.activeBackground": "#f16e2d",
    "titleBar.activeForeground": "#000000",
    "titleBar.inactiveBackground": "#81492d",
    "titleBar.inactiveForeground": "#676767"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.fixAll.eslint": "explicit"
  },
  "eslint.validate": ["typescript", "typescriptreact", "vue"],
  "[typescript, typescript, json]": {
    "editor.tabSize": 2,
    "editor.rulers": [72, 80, 100]
  },
  // "editor.insertSpaces": true,
  "editor.tabSize": 2,
  "editor.stickyScroll.enabled": true,
  "sqltools.connections": [
    {
      "previewLimit": 50,
      "server": "localhost",
      "driver": "PostgreSQL",
      "connectString": "postgres://postgres:postgres@localhost:5432/postgres",
      "name": "postgres"
    }
  ],

}
